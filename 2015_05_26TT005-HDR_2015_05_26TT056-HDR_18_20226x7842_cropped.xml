<krpano version="1.18">
	
    <!-- <include url="skin/defaultskin.xml" /> -->
	
	<plugin 
        name="gyro" 
        devices="html5" 
        url="plugins/gyro.js" 
        enabled="true" 
        camroll="true" 
        friction="0.4" 
        velastic="0.8" 
		onloaded="delayedcall(1,ifnot(available, showgyrowarning() ));" />
	
	<!-- view settings -->
	<view 
        hlookat="38" 
        vlookat="10" 
        maxpixelzoom="2.0"
        fovmax="100" 
        limitview="range" 
        vlookatmin="-69.790" 
        vlookatmax="69.790" />
	
	<preview url="2015_05_26TT005-HDR_2015_05_26TT056-HDR_18_20226x7842_cropped.tiles/preview.jpg" />
	<image>
		<cube url="2015_05_26TT005-HDR_2015_05_26TT056-HDR_18_20226x7842_cropped.tiles/pano_%s.jpg" />
		<mobile>
			<cube url="2015_05_26TT005-HDR_2015_05_26TT056-HDR_18_20226x7842_cropped.tiles/mobile_%s.jpg" />
		</mobile>
	</image>

    <hotspot 
        name="desk"
        type="image"
        url="images/info-icon.png"
        visible="true"
        enabled="true"
        ath="161"
        atv="168"
        scale="0.5"
        edge="center"
        zoom="true"
        onclick="js( popModal('desk') );" />

    <hotspot 
        name="carpet"
        type="image"
        url="images/info-icon.png"
        visible="true"
        enabled="true"
        ath="218"
        atv="140"
        scale="0.5"
        edge="center"
        zoom="true"
        onclick="js( popModal('carpet') );" />

